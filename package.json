{
  "name": "graphql-typescript-server-template",
  "version": "0.0.1",
  "description": "A template for GraphQL-supported TypeScript server",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prod:build": "npm run clean && webpack -p",
    "dev:build": "npm run clean && webpack -d",
    "dev:watch": "npm run clean && webpack -d --watch",
    "start": "rimraf dist && webpack && node dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kekland/graphql-typescript-server-template.git"
  },
  "keywords": [
    "graphql",
    "typescript",
    "template"
  ],
  "author": "kekland <kk.erzhan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kekland/graphql-typescript-server-template/issues"
  },
  "homepage": "https://github.com/kekland/graphql-typescript-server-template#readme",
  "dependencies": {
    "apollo-server": "^2.19.0",
    "chalk": "^2.4.2",
    "class-validator": "^0.12.2",
    "graphql": "^15.4.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.1.2",
    "ts-node": "^8.4.1",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
